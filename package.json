{
  "name": "electron-super-simple-updater",
  "version": "0.1.0",
  "description": "A super simple local updater for Electron, just with replacing app.asar",
  "main": "index.js",
  "scripts": {
    "dev": "electron .",
    "pkg": "electron-packager . Updater --platform=win32,darwin --arch=ia32,x64 --asar --overwrite --out=appdist --app-bundle-id=com.sanrokugomaato.updater --icon=icon/icon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanrokugomaato/electron-super-simple-updater.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanrokugomaato/electron-super-simple-updater/issues"
  },
  "homepage": "https://github.com/sanrokugomaato/electron-super-simple-updater",
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2"
  }
}
